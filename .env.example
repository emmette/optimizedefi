# 1inch API Configuration
ONEINCH_API_KEY=your_api_key_here

# Backend Configuration
BACKEND_PORT=8000
BACKEND_HOST=0.0.0.0

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000

# Database Configuration (if needed)
DATABASE_URL=postgresql://user:password@localhost:5432/optimizedefi

# OpenRouter Configuration (Required for AI agents)
# Get your API key from https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Model Configuration (Optional - defaults shown)
# See available models at: https://openrouter.ai/models
# DEFAULT_MODEL=google/gemini-2.0-flash
# PORTFOLIO_AGENT_MODEL=google/gemini-2.0-flash
# REBALANCING_AGENT_MODEL=openai/gpt-4o
# SWAP_AGENT_MODEL=google/gemini-2.0-flash
# GENERAL_AGENT_MODEL=google/gemini-2.0-flash

# JWT Configuration
JWT_SECRET=your-secret-key-here-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# Network Configuration
NEXT_PUBLIC_SUPPORTED_CHAINS=1,137,10,42161

# Environment
NODE_ENV=development